{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-flow-strip-types",
    "react-hot-loader/babel",
    ["import", {
      "libraryName": "antd",
      "style": true
    }]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types", 
        "transform-react-constant-elements",
        ["strip-function-call", {
          "strip": [
            "Perf.start",
            "Perf.stop",
            "Perf.printExclusive",
            "Perf.printInclusive",
            "Perf.getLastMeasurements",
            "console.log"
          ]
        }]
      ]
    },
    "test": {
      "plugins": [
        [ "babel-plugin-webpack-alias", { "config": "./config/webpack.config.base.js" } ]
      ]
    }
  }
}
