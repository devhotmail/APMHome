<#setting number_format="0">
<#setting datetime_format="yyyy-MM-dd"/>
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:ris="http://ris.webservice.dragon.hcit.ge.com">
   <soapenv:Header/>
   <soapenv:Body>
      <ris:${headers.SOAPAction?replace('"','')}>
         <#if headers['empiID']??><empiID>${headers['empiID']?default("")}</empiID></#if>
         <inPatientID>${headers['inPatientID']?default("")}</inPatientID>
         <outPatientID>${headers['outPatientID']?default("")}</outPatientID>
         <patientAddress>${headers['patientAddress']?default("")}</patientAddress>
         <#if headers['patientBirthDate']?default("")!=''><patientBirthDate>${headers['patientBirthDate']}</patientBirthDate></#if>
         <patientGender>${headers['patientGender']?default("")}</patientGender>
         <patientIdentityCardId>${headers['patientIdentityCardId']?default("")}</patientIdentityCardId>
         <patientMedicareCardId>${headers['patientMedicareCardId']?default("")}</patientMedicareCardId>
         <patientName>${headers['patientName']?default("")}</patientName>
         <patientPhoneNumber>${headers['patientPhoneNumber']?default("")}</patientPhoneNumber>
         <patientPostalCode>${headers['patientPostalCode']?default("")}</patientPostalCode>
      </ris:${headers.SOAPAction?replace('"','')}>
   </soapenv:Body>
</soapenv:Envelope>